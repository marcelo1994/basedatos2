CREATE TABLE libros
(
libro_id INTEGER,
isbn VARCHAR2(13),
titulo VARCHAR2(200),
resumen VARCHAR2(2000),
autor VARCHAR2(200),
fecha_publicacion DATE,
num_pag NUMBER,
CONSTRAINT pk_book_id PRIMARY KEY (book_id)
) AS 
SET serveroutput ON;
CREATE SEQUENCE SQ_LIBRO
START WITH 1
INCREMENT BY 1
nomaxvalue;


